class SwitchCaseTest {
	public static void main(String[] args) {
		int num = 1;
		swith(num){

			case 0:
				System.out.println("zero");
			case 1:
				System.out.println("one");
			case 2:
				System.out.println("two");
			case 3:
				System.out.println("three");
			default:
				System.out.println("other");
		}
	}
}
